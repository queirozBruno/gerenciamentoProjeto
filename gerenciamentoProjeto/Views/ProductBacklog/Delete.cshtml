@model Modelo.Tabelas.ProductBacklog

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <h3>Deseja realmente excluir este produto ?</h3>
    <div>
        <h4>Product Backlog</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayName("Nome do Produto")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.ProductBacklogNome)
            </dd>
    
            <dt>
                @Html.DisplayName("Prioridade")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.ProductBacklogPrioridade)
            </dd>
    
            <dt>
                @Html.DisplayName("Descrição")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.ProductBacklogDescricao)
            </dd>
    
            <dt>
                @Html.DisplayName("Notas")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.ProductBacklogNotas)
            </dd>
    
        </dl>
    
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()
    
            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-default" /> |
                @Html.ActionLink("Back to List", "Index")
            </div>
        }
    </div>
</body>
</html>
